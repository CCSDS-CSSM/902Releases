 FR and the parameter value must be of the same type as the parameter value that
 shall be set.
-->
  <xsd:complexType name="RngAndDopplerExtractionContrParamIdsAndValuesDirQual">
    <xsd:complexContent>
      <xsd:extension base="DirectiveQualifier"/>
    </xsd:complexContent>
  </xsd:complexType>
  
</xsd:schema>
