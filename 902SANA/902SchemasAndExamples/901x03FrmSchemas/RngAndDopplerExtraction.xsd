        <xsd:attribute fixed="1.3.112.4.4.2.1.20301.1.15.1.1" name="oid" type="ObjectIdentifier"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
   
  <xsd:simpleType name="RngAndDopplerExtractionRngMeasurementSamplingRate">
    <xsd:restriction base="xsd:integer">
